{
  "name": "tuto-api",
  "version": "1.0.0",
  "description": "API REST",
  "main": "main.js",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "test": "mocha --watch --require ts-node/register --watch-extension ts test/**/*.spec.ts",
    "typeorm": "ts-node ./node_modules/typeorm/cli -f ./ormconfig.json",
    "db:sync" : "npm run typeorm schema:drop && npm run typeorm schema:sync"
  },
  "keywords": [
    "API"
  ],
  "author": "Jonathan Sylvestre",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.29"
  },
  "devDependencies": {
    "@types/chai-http": "^4.2.0",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.9",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.14",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
